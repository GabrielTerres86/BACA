                                                   ,rr_craplot.cdhistor
                                                   ,rr_craplot.nrdcaixa
                                                   ,rr_craplot.cdoperad) RETURNING ROWID INTO vr_rowid;
        
      END IF;
        pr_craplot_rowid(vr_index_craplot_rowid).vr_rowid := vr_rowid;
      end loop rr_craplot;
  EXCEPTION
    WHEN OTHERS THEN
      pr_dserro :=  'pc_gerar_lote_from_wrk '||vr_dslog ||' Sqlerrm: '||sqlerrm;
        
