CREATE OR REPLACE TRIGGER CECRED.TRG_taa_lim_saq_PROG_RECID
BEFORE INSERT ON tbtaa_limite_saque

FOR EACH ROW

BEGIN

  IF :NEW.PROGRESS_RECID IS NULL THEN
    :NEW.PROGRESS_RECID := tbtaa_limite_saque_SEQ.NEXTVAL;
  END IF;

EXCEPTION
  WHEN OTHERS THEN
    RAISE_APPLICATION_ERROR(-20100,'ERRO GERACAO SEQUENCE PROGRESS_RECID - TABELA tbtaa_limite_saque!');

END;
/

