CREATE OR REPLACE TRIGGER CECRED."TRG_EPR_COBRANCA_PROG_RECID"
BEFORE INSERT ON tbepr_cobranca

FOR EACH ROW

BEGIN

  IF :NEW.PROGRESS_RECID IS NULL THEN
    :NEW.PROGRESS_RECID := tbepr_cobranca_SEQ.NEXTVAL;
  END IF;

EXCEPTION
  WHEN OTHERS THEN
    RAISE_APPLICATION_ERROR(-20100,'ERRO GERACAO SEQUENCE PROGRESS_RECID - TABELA tbepr_cobranca_SEQ!');

END;
/

