CREATE OR REPLACE TRIGGER CECRED.TRG_CRAPTEX_PROGRESS_RECID
BEFORE INSERT ON CRAPTEX

FOR EACH ROW

BEGIN

  IF :NEW.PROGRESS_RECID IS NULL THEN
    :NEW.PROGRESS_RECID := CRAPTEX_SEQ.NEXTVAL;
  END IF;

EXCEPTION
  WHEN OTHERS THEN
    RAISE_APPLICATION_ERROR(-20100,'ERRO GERACAO SEQUENCE PROGRESS_RECID - TABELA CRAPTEX!');

END;
/

