CREATE OR REPLACE TRIGGER CECRED.TRG_TBCOBRAN_ASSESS_PRGRECID
BEFORE INSERT ON TBCOBRAN_ASSESSORIAS

FOR EACH ROW

BEGIN

  IF :NEW.PROGRESS_RECID IS NULL THEN
    :NEW.PROGRESS_RECID := TBCOBRAN_ASSESSORIAS_SEQ.NEXTVAL;
  END IF;

EXCEPTION
  WHEN OTHERS THEN
    RAISE_APPLICATION_ERROR(-20100,'ERRO GERACAO SEQUENCE PROGRESS_RECID - TABELA TBCOBRAN_ASSESSORIAS!');

END;
/

