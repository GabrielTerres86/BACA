CREATE OR REPLACE TRIGGER CECRED.TBCOBRAN_MOTIVOS_CIN_PRGRECID
BEFORE INSERT ON TBCOBRAN_MOTIVOS_CIN

FOR EACH ROW

BEGIN

  IF :NEW.PROGRESS_RECID IS NULL THEN
    :NEW.PROGRESS_RECID := TBCOBRAN_MOTIVOS_CIN_SEQ.NEXTVAL;
  END IF;

EXCEPTION
  WHEN OTHERS THEN
    RAISE_APPLICATION_ERROR(-20100,'ERRO GERACAO SEQUENCE PROGRESS_RECID - TABELA TBCOBRAN_MOTIVOS_CIN!');

END;
/

