CREATE OR REPLACE TRIGGER CECRED.TRG_CID_ATUACAO_COOP_RECID
BEFORE INSERT ON tbgen_CID_ATUACAO_COOP

FOR EACH ROW

BEGIN

  IF :NEW.PROGRESS_RECID IS NULL THEN
    :NEW.PROGRESS_RECID := tbgen_cid_atuacao_coop_seq.NEXTVAL;
  END IF;

EXCEPTION
  WHEN OTHERS THEN
    RAISE_APPLICATION_ERROR(-20100,'ERRO GERACAO SEQUENCE PROGRESS_RECID - TABELA tbgen_cid_atuacao_coop!');

END;
/

