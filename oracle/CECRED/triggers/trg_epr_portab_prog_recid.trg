CREATE OR REPLACE TRIGGER CECRED."TRG_epr_portab_PROG_RECID"
BEFORE INSERT ON tbepr_portabilidade

FOR EACH ROW

BEGIN

  IF :NEW.PROGRESS_RECID IS NULL THEN
    :NEW.PROGRESS_RECID := tbepr_portabilidade_SEQ.NEXTVAL;
  END IF;

EXCEPTION
  WHEN OTHERS THEN
    RAISE_APPLICATION_ERROR(-20100,'ERRO GERACAO SEQUENCE PROGRESS_RECID - TABELA tbepr_portabilidade!');

END;
/

