CREATE OR REPLACE TRIGGER CECRED.TRG_CRAPACE_PROGRESS_RECID
BEFORE INSERT ON CRAPACE

FOR EACH ROW

BEGIN

  IF :NEW.PROGRESS_RECID IS NULL THEN
    :NEW.PROGRESS_RECID := CRAPACE_SEQ.NEXTVAL;
  END IF;

EXCEPTION
  WHEN OTHERS THEN
    RAISE_APPLICATION_ERROR(-20100,'ERRO GERACAO SEQUENCE PROGRESS_RECID - TABELA CRAPACE!');

END;
/

