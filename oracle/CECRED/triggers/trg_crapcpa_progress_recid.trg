CREATE OR REPLACE TRIGGER CECRED.TRG_CRAPCPA_PROGRESS_RECID
BEFORE INSERT ON CRAPCPA

FOR EACH ROW

BEGIN

  IF :NEW.PROGRESS_RECID IS NULL THEN
    :NEW.PROGRESS_RECID := CRAPCPA_SEQ.NEXTVAL;
  END IF;

EXCEPTION
  WHEN OTHERS THEN
    RAISE_APPLICATION_ERROR(-20100,'ERRO GERACAO SEQUENCE PROGRESS_RECID - TABELA CRAPCPA!');

END;
/

