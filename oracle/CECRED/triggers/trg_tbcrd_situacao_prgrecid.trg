CREATE OR REPLACE TRIGGER CECRED.TRG_TBCRD_SITUACAO_PRGRECID
BEFORE INSERT ON TBCRD_SITUACAO

FOR EACH ROW

BEGIN

  IF :NEW.PROGRESS_RECID IS NULL THEN
    :NEW.PROGRESS_RECID := TBCRD_SITUACAO_SEQ.NEXTVAL;
  END IF;

EXCEPTION
  WHEN OTHERS THEN
    RAISE_APPLICATION_ERROR(-20100,'ERRO GERACAO SEQUENCE PROGRESS_RECID - TABELA TBCRD_SITUACAO!');

END;
/

