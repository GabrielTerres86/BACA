<?xml version="1.0" encoding="UTF-8"?>
<fontFamilies>
    <fontFamily name="Arial">
        <normal><![CDATA[Arial.ttf]]></normal>
        <bold><![CDATA[Arial_Bold.ttf]]></bold>
        <italic><![CDATA[Arial_Italic.ttf]]></italic>
        <boldItalic><![CDATA[Arial_Bold_Italic.ttf]]></boldItalic>
        <pdfEmbedded><![CDATA[true]]></pdfEmbedded>
        <exportFonts>
               <export key="net.sf.jasperreports.xhtml"><![CDATA["Arial", Helvetica, sans-serif]]></export>
               <export key="net.sf.jasperreports.html"><![CDATA["Arial", Helvetica, sans-serif]]></export>
       </exportFonts>
    </fontFamily>
    <fontFamily name="Courier New">
        <normal><![CDATA[Courier_New.ttf]]></normal>
        <bold><![CDATA[Courier_New_Bold.ttf]]></bold>
        <italic><![CDATA[Courier_New_Italic.ttf]]></italic>
        <boldItalic><![CDATA[Courier_New_Italic.ttf]]></boldItalic>
        <pdfEmbedded><![CDATA[true]]></pdfEmbedded>
        <exportFonts>
               <export key="net.sf.jasperreports.xhtml"><![CDATA["Courier New", Courier, monospace]]></export>
               <export key="net.sf.jasperreports.html"><![CDATA["Courier New", Courier, monospace]]></export>
       </exportFonts>
    </fontFamily>
    
    <!-- Mapping Java logical fonts, for the sake of HTML export only. -->	
    <fontFamily name="SansSerif">
        <exportFonts>
            <export key="net.sf.jasperreports.html">'Arial', Helvetica, sans-serif</export>
            <export key="net.sf.jasperreports.xhtml">'Arial', Helvetica, sans-serif</export>
        </exportFonts>
    </fontFamily>
    <fontFamily name="Monospaced">
        <exportFonts>
            <export key="net.sf.jasperreports.html">'Courier New', Courier, monospace</export>
            <export key="net.sf.jasperreports.xhtml">'Courier New', Courier, monospace</export>
        </exportFonts>
    </fontFamily>
</fontFamilies>