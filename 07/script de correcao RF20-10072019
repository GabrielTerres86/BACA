--1-Conta 8253552 cooperativa 1 - correção de ch no vlr 359,80

UPDATE crapsda t 
SET t.vlsdbloq = t.vlsdbloq + 359.80 --Quando o bloqueio é negativo
   ,t.vlsddisp = t.vlsddisp - 359.80 --Quando o disponível é posiivo 
 WHERE t.cdcooper = 1
   AND t.nrdconta = 8253552
   AND t.dtmvtolt = '09/07/2019';

commit;

 UPDATE crapsld t 
    SET t.vlsdbloq = t.vlsdbloq + 359.80
       ,t.vlsddisp = t.vlsddisp - 359.80 
  WHERE t.cdcooper = 1
    AND t.nrdconta = 8253552;

commit;
-----------------------------------------------------

