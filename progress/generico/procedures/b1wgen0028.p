                                                      INPUT par_cdoperad,
                                                      INPUT par_idorigem,
                                                      INPUT "CARTAO CRED.",
                                                      INPUT par_nrdconta,
                                                      INPUT par_dtmvtolt,
                                                      INPUT 4, /* Tipo Contrato */
