    ELSE
        IF  f_verifica_adm(crapadc.cdadmcrd) = 3 THEN
           DO:
             /*VALIDANDO O BOTÃO HABILITAR PRA CARTÕES BRADESCO*/
             IF crapass.inpessoa = 2  THEN
                       DO:
            
                               FIND craphcj WHERE craphcj.cdcooper = par_cdcooper   AND
