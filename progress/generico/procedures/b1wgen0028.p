                                                   INPUT par_nrdcaixa,
                                                   INPUT par_idorigem,
                                                   INPUT par_nrdconta,
                                                   INPUT par_dtmvtolt,
                                                   INPUT par_nrctaava,
                                                   INPUT par_nrcpfcgc,
                                                  OUTPUT TABLE tt-dados-avais,
                                                  OUTPUT TABLE tt-erro).
                
